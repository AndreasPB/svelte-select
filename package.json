{
  "name": "svelte-select",
  "version": "5.0.0-alpha.5",
  "repository": "https://rob-balfre@github.com/rob-balfre/svelte-select.git",
  "description": "A <Select> component for Svelte apps",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "dev-tests": "rollup -cw",
    "test:browser": "sirv test/public -p 3000 --dev",
    "gen:docs": "node docs/generate_theming_variables_md.cjs",
    "prepare": "svelte-kit package",
    "release": "dotenv release-it --dry-run"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.196",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.1.1",
    "find-in-files": "^0.5.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.5.0",
    "release-it": "^14.11.8",
    "rollup": "^2.60.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.44.1",
    "svelte-preprocess": "^4.9.8",
    "svelte-tiny-virtual-list": "^1.1.7",
    "svelte2tsx": "^0.4.8",
    "tailwindcss": "^2.2.19",
    "tape-modern": "^1.1.2",
    "typescript": "^4.4.4"
  },
  "author": "Robert Balfr√© <rob.balfre@gmail.com> (https://github.com/rob-balfre)",
  "license": "ISC",
  "keywords": [
    "svelte"
  ],
  "release-it": {
    "hooks": {
      "before:init": [
        "svelte-kit package && cd package"
      ]
    },
    "github": {
      "release": true
    }
  },
  "type": "module"
}
